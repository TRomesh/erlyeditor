    "eslint-plugin-filenames": "0.2.0",
    "eslint-plugin-import": "0.12.1",
    "eslint-plugin-react": "3.16.1",
    "exact-semver": "1.2.0",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.8.5",
    "fixpack": "2.2.0",
    "ghooks": "1.0.3",
